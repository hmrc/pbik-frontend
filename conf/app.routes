# Microservice specific routes
GET         /public/*file                                        controllers.Assets.versioned(path="/public", file: Asset)
GET         /assets/*file                                        controllers.Assets.versioned(path="/public", file: Asset)

## Point old urls to new dashboard url
GET         /                                                    controllers.RedirectController.redirectIfFromRoot
GET         /start                                               controllers.RedirectController.redirectIfFromStart
GET         /overview                                            controllers.RedirectController.redirectIfFromOldOverview
GET         /payrolled-benefits-expenses                         controllers.RedirectController.redirectIfFromOldOverview

GET         /start-payrolling-benefits-expenses                  controllers.StartPageController.onPageLoad

GET         /not-authorised                                      controllers.AuthController.notAuthorised

GET         /signout                                             controllers.HomePageController.signout

GET         /setLanguage                                         controllers.HomePageController.setLanguage

GET         /registered-benefits-expenses                        controllers.HomePageController.onPageLoad

## Add registrations
GET         /cy1/choose-benefit-expense                          controllers.registration.ManageRegistrationController.nextTaxYearAddOnPageLoad
POST        /cy1/choose-benefit-expense                          controllers.registration.ManageRegistrationController.checkYourAnswersAddNextTaxYear

GET         /cy1/check-the-benefits                             controllers.registration.ManageRegistrationController.showCheckYourAnswersAddNextTaxYear
POST        /cy1/check-the-benefits                             controllers.registration.ManageRegistrationController.addNextYearRegisteredBenefitTypes

GET         /cy/choose-benefit-expense                           controllers.registration.ManageRegistrationController.currentTaxYearOnPageLoad
POST        /cy/choose-benefit-expense                           controllers.registration.ManageRegistrationController.checkYourAnswersAddCurrentTaxYear

GET         /cy/check-the-benefits                              controllers.registration.ManageRegistrationController.showCheckYourAnswersAddCurrentTaxYear
POST        /cy/check-the-benefits                              controllers.registration.ManageRegistrationController.updateCurrentYearRegisteredBenefitTypes

GET         /:year/registration-complete                         controllers.WhatNextPageController.showWhatNextRegisteredBik(year: String)


## Remove registrations
GET         /cy1/:iabdType/remove-benefit-expense                controllers.registration.ManageRegistrationController.checkYourAnswersRemoveNextTaxYear(iabdType: String)
POST        /cy1/:iabdType/remove-benefit-expense                controllers.registration.ManageRegistrationController.removeNextYearRegisteredBenefitTypes(iabdType: String)

GET         /cy1/:iabdType/benefit-removed                       controllers.WhatNextPageController.showWhatNextRemovedBik(iabdType: String)

## Manage exclusion list
GET         /:year/:iabdType/excluded-employees                  controllers.ExclusionListController.performPageLoad(year: String, iabdType: String)
POST        /:year/:iabdType/excluded-employees                  controllers.ExclusionListController.submitExcludedEmployees(year: String, iabdType: String)

GET         /:year/:iabdType/employee-national-insurance-number  controllers.ExclusionListController.withOrWithoutNinoOnPageLoad(year: String, iabdType: String)
POST        /:year/:iabdType/employee-national-insurance-number  controllers.ExclusionListController.withOrWithoutNinoDecision(year: String, iabdType: String)

GET         /:year/:iabdType/:formType/search-for-employee       controllers.ExclusionListController.showExclusionSearchForm(year: String, iabdType: String, formType: String)
POST        /:year/:iabdType/:formType/search-for-employee       controllers.ExclusionListController.searchResults(year: String, iabdType: String, formType: String)
GET         /:year/:iabdType/:formType/exclude-employee-results  controllers.ExclusionListController.showResults(year: String, iabdType: String, formType: String)
POST        /:year/:iabdType/:formType/exclude-employee-results  controllers.ExclusionListController.updateMultipleExclusions(year: String, iabdType: String, formType: String)
GET         /:year/:iabdType/exclude-employee-results-single     controllers.ExclusionListController.updateExclusions(year: String, iabdType: String)

GET         /:year/:iabdType/exclusion-complete                  controllers.ExclusionListController.showExclusionConfirmation(year: String, iabdType: String)

GET         /:year/:iabdType/check-employee-details              controllers.ExclusionListController.showRemovalConfirmation(year: String, iabdType: String)
GET         /:year/:iabdType/exclude-employee-remove/:nino       controllers.ExclusionListController.remove(year: String, iabdType: String, nino: String)
GET         /:iabdType/employee-registration-complete            controllers.ExclusionListController.showRemovalWhatsNext(iabdType: String)
POST        /:iabdType/employee-registration-complete            controllers.ExclusionListController.removeExclusionsCommit(iabdType: String)

